# Mendefinisikan set
A = {'a', 'b', 'c', 'd'}
B = {'c', 'd', 'e'}
C = set()

#Menampilkan data set awal yang telah ditentukan
print("Set awal:")
print("A =", A)
print("B =", B)
print("C =", C)

print("\nHasil operasi difference:")
#soal point a(A difference B)
print("a. A difference B =", A.difference(B))

#soal point b(B difference A)
print("b. B difference A =", B.difference(A))

#soal point c(A difference C)
print("c. A difference C =", A.difference(C))

#soal point d(B difference C)
print("d. B difference C =", B.difference(C))
